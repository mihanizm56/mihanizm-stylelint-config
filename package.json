{
  "name": "@wildberries/stylelint-config",
  "version": "2.1.6",
  "description": "Wildberries seller stylelint",
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "commitizen": "4.0.4",
    "cz-customizable": "6.2.0",
    "husky": "3.0.9"
  },
  "dependencies": {
    "stylelint-config-recommended-scss": "7.0.0",
    "stylelint-config-standard": "26.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "stylelint": "14.9.1",
    "stylelint-max-lines": "0.1.0"
  },
  "peerDependencies": {
    "postcss": "8.4.14"
  },
  "scripts": {
    "commit": "git add . && git commit --no-edit",
    "release": "npm run commit && git push && npm publish --access=public",
    "beta": "npm run commit && git push && npm publish --tag beta --access=public"
  },
  "keywords": [],
  "author": "mihanizm56",
  "license": "MIT",
  "main": "index.js",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g config/git/commitlint.js",
      "prepare-commit-msg": "node config/git/prepare-commit.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}